            <!-- {% else %} 
            <img style="width: 260px;float: left;margin: 5px 10px;" src="/images/noimage.jpg"/>
            {% endif %}-->
             {{ newsitem.content }}

            </div>
          </div>
        </div>
        <div class="clear"></div>
      <div class="row">
        <div class="ExperModule" style="width: 92%;margin-left: 30px;">
         
            {% if newsitem.linked_text_articles %}
               <div class="module">
              <div class="module-title">Related Articles</div>
              <div class="module-inner">
                {% for article in newsitem.linked_text_articles %}
               <div class="post">
                  <a ng-href="/n/{{ article.id }}"><h4 style="font-weight: normal;font-size: 17px;">{{ article.title }}</h4></a>
                  {{ article.view_date }}</span>
               </div>
               {% endfor %}

              </div>
               </div>
            {% endif %}
         
        </div>
        <!--ExperModule End-->
      </div>
        <!--ExperModule End-->
        
        <div class="clear"></div>

        <!-- <div class="row">
                <div class="ExperModulead" >
                  <div class="module">            
                    <div class="module-inner">
                        <div class="rightad2" style="margin:0 auto; width:300px;"><script src="http://tag.contextweb.com/TagPublish/getjs.aspx?action=VIEWAD&cwrun=200&cwadformat=300X250&cwpid=557677&cwwidth=300&cwheight=250&cwpnet=1&cwtagid=213296"></script></div>
                    </div>
                  </div>
                </div>
        </div> -->
        
      <div class="row">
        <div class="ExperModule" style="width: 92%;margin-left: 30px;">
          <div class="module">
            <div class="module-title">More Fitness News</div>
            <div class="module-inner">
           
              {% for news_item in topnews%}
             <div class="post">
              <!-- {% if news_item.thumb_image %} -->
                <a href="/n/{{ news_item.id }}">
                <!-- <img style="float: left;margin-right: 10px;height: 105px;width: 150px;" src="http://img1.contentapi.ws/{{ news_item.thumb_image}}"/> -->
                <!-- {% else %}
                <img style="float: left;margin-right: 10px;height: 105px;width: 150px;" src="/images/noimage.jpg"/> -->
                </a>
                <!-- {% endif %} -->
                <a href="/n/{{ news_item.id }}"><h4>{{ news_item.title }}</h4></a>
                <p class="postdescription">{{ news_item.description }}...<a href="/n/{{ news_item.id }}">Read more</a></p>
                <span class="postby" style="float:right;">{{ news_item.upload_date }}</span>
             </div>
             {% endfor %}
             <div style="float: right;margin-right: 3%;"><a href="/morefitnessnews?p=1">more news</a></div>
            </div>
            <span class="clearboth"></span>
              <span class="clearboth"></span>
              <!-- <div class="row" style="margin-left: 44px;">
                <pagination on-select-page="pageChanged(page)" items-per-page="newsPerPage" total-items="bigTotalItems" page="bigCurrentPage" max-size="maxSize" boundary-links="true" rotate="false" num-pages="numPages"></pagination>
              </div> -->
          </div>

        </div>
