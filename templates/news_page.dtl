<script>
      function epochToDate(epochSecs) { 
        var m_names = new Array('Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec'); 
        var d = new Date(new Date(0).setUTCSeconds(parseInt(epochSecs))); 
        return (m_names[d.getMonth()]+ " " + d.getDate()+ " " + d.getFullYear()); 
      }
    </script>
<style>
.uploaded-date{
  font-weight: normal;
  font-size: 12px;
  font-style: italic;
}
.module-title {
  font-size: 16px;
  text-transform: uppercase;
  color: #000a11;
  font-weight: 800;
  padding: 6px 10px;
  background: #ddd;
}
.newsbody{
  text-align: justify;
line-height: 23px;
}
</style>
</head>


<body>
{% include "header.dtl" %}
<!--Navgation End-->
<div class="container2">
  <article>
    <aside class="width67">          
      <div class="clear"></div>
      <div class="row">         
        <div class="ExperModule" style="width: 96%;">
          <div class="module">
            <div class="newsbody">
              <div class="module-title">{{ newsitem.title }}&nbsp;&nbsp;<span class="uploaded-date">-{{ newsitem.view_date }}</span></div>
              
             <!-- {% if newsitem.thumb_image %} -->
